{
    "version": 2,
    "builds": [
        {
            "src": "backend/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/index.js"
        }
    ],
    "functions": {
        "backend/index.js": {
            "memory": 1024,
            "maxDuration": 20
        }
    }
}